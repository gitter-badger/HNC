allDigits ss = {
	lt c d = less c d
	leq c d = _or (lt c d) (eq c d)
	gt c d = _not (leq c d)
	geq c d = _not (lt c d)
	xand a b = _not (_or (_not a) (_not b))
	between a b c = xand (geq a b) (lt b c)
	isDigit c = between 48 c 58
	loopCond s = isDigit (deref s)
	loopBody s = next s
	firstFailure = whileF loopCond loopBody ss
	eq (deref firstFailure) 0
}