allDigits ss = {
	lt c d = less c d
	leq c d = _or (lt c d) (eq c d)
	gt c d = _not (leq c d)
	geq c d = _not (lt c d)
	xand a b = _not (_or (_not a) (_not b))
	between a b c = xand (geq a b) (lt b c)
	isDigit c = between 48 c 58
	comp f g = {
		h x = f (g x)
		h
	}
	firstFailure = whileF (comp isDigit deref) next ss
	eq (deref firstFailure) 0
}